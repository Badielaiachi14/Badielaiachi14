class acc{
    String account;
    int number;

    public String getAccount() {
        return account;
    }

    public int getNumber() {
        return number;
    }

    public void setNumber(int number) {
        this.number = number;
    }

    public void setAccount(String account) {
        this.account = account;
    }
}
class Main{
    public static void main(String[]args){
        acc f5 = new acc();
        f5.setNumber(1234098761);
        System.out.println(f5.getNumber());
        f5.setAccount("ICIC BANK");
        System.out.println(f5.getAccount());
    }
}


// another code 




class A{
    private int Num;

    public int getNum() {
        return Num;
    }

    public void setNum(int Num) {
        this.Num = Num;
    }
}
public class Main{
    public static void main(String[]args){
        A some = new A();
        some.setNum(100);
        System.out.println(some.getNum());
    }
}
