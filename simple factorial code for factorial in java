import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int n, a =1;
        System.out.println("enter your number:");
        Scanner scanner = new Scanner(System.in);
        n = scanner.nextInt();
        for (int i =1; i<=n;i++){
            a= a * i;
        }
        System.out.println("factorial:"+ a);
    }
}
